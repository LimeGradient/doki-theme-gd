{
	"geode": "3.8.1",
	"gd": {
		"android": "2.206",
		"win": "2.206",
		"mac": "2.206"
	},
	"version": "v1.4.0",
	"id": "limegradient.doki-theme-gd",
	"name": "Doki Theme",
	"developers": [
		"LimeGradient",
		"RayDeeUx"
	],
	"description": "Add Anime Characters to Geometry Dash!",
	"links": {
		"community": "https://discord.gg/44ANAhXz7r",
		"source": "https://github.com/LimeGradient/doki-theme-gd"
	},
	"resources": {
		"sprites": [
			"resources/background/*.png",
			"resources/stickers/*.png"
		]
	},
"settings": {
		"custom-sticker-title": {
			"type": "title",
			"name": "Custom Stickers"
		},
		"CustomStickerPath": {
			"name": "Custom Sticker Path",
			"description": "Choose a custom sticker to display",
			"type": "file",
			"default": ""
		},
		"CustomBackgroundPath": {
			"name": "Custom Background Path",
			"description": "Choose a background to display",
			"type": "file",
			"default": ""
		},
		"UseCustomSticker": {
			"name": "Use Custom Sticker",
			"description": "Override the Sticker Slider and use a custom sticker",
			"type": "bool",
			"default": false
		},
		"UseCustomBackground": {
			"name": "Use Custom Background",
			"description": "Override the Background Slider and use a custom background",
			"type": "bool",
			"default": false
		},
		"built-in-stickers": {
			"type": "title",
			"name": "Built-In Stickers"
		},
		"StickerSlider": {
			"name": "Sticker",
			"description": "Choose a sticker from here.",
			"type": "string",
			"default": "Essex",
			"one-of": ["Essex", "Sleepy Kanna", "Kanna", "Mai", "Nagatoro", "Ram", "Reggie", "Rem", "Rias", "Ryuko", "Yuno"]
		},
		"BackgroundSlider": {
			"name": "Background (Editor Only)",
			"description": "Choose a level editor background from here.",
			"type": "string",
			"default": "Aqua",
			"one-of": ["Aqua", "Astolfo", "Asuna", "Azuki", "Beatrice", "Cinnamon", "Coconut", "Darkness", "Echidna", "Gray", "Rias", "Rin", "Ryuko", "Sayori", "Soma", "Tohru", "Umi", "V8 Girls", "Yuri", "Zero Two"]
		},
		"sticker-settings": {
			"type": "title",
			"name": "Sticker Settings"
		},
		"TransparencySliderLevel": {
			"name": "Sticker Transparency",
			"type": "int",
			"default": 100,
			"min": 10,
			"max": 100,
			"control": {
				"slider": true
			}
		},
		"ShowStickerInLevel": {
			"name": "Show Sticker in Level",
			"type": "bool",
			"default": true
		},
		"ShowStickerInEditor": {
			"name": "Show Sticker in Editor",
			"type": "bool",
			"default": true
		},
		"xPosPercent": {
			"name": "Sticker X Position (%)",
			"description": "Sets the position of the sticker as a percentage of the width of your screen.",
			"type": "int",
			"default": 90,
			"min": 0,
			"max": 100
		},
		"yPosPercent": {
			"name": "Sticker Y Position (%)",
			"description": "Sets the position of the sticker as a percentage of the height of your screen.",
			"type": "int",
			"default": 10,
			"min": 0,
			"max": 100
		},
		"Scale": {
			"name": "Scale",
			"description": "Sets the scale of the sticker.",
			"type": "float",
			"default": 1.0,
			"min": 0.1,
			"max": 2,
			"control": {
				"slider": true,
				"input": true,
				"arrows": false,
				"slider-step": 0.01
			}
		},
		"TransparencySliderEditor": {
			"name": "Background Transparency",
			"type": "int",
			"default": 75,
			"min": 10,
			"max": 100,
			"control": {
				"slider": true
			}
		},
		"ShowBackgroundInEditor": {
			"name": "Show Background in Editor",
			"type": "bool",
			"default": true
		},
		"BackgroundXPos": {
			"name": "Background X Position",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 500,
			"control": {
				"slider": true
			}
		},
		"BackgroundYPos": {
			"name": "Background Y Position",
			"type": "int",
			"default": 0,
			"min": 0,
			"max": 500,
			"control": {
				"slider": true
			}
		},
		"BackgroundScale": {
			"name": "Background Scale",
			"type": "float",
			"default": 1,
			"min": 1,
			"max": 10,
			"control": {
				"slider": true
			}
		}
	},
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": ">=1.13.0",
			"importance": "required"
		}
	]
}
